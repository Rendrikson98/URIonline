#include <stdio.h>
#include <stdlib.h>

int main()
{
    int x;
    while(1){
        printf("Digite um valor:");
        scanf("%d", &x);
        if (x==0) break;
        else{
            for(int i = 1; i<=x; i++){
                printf("%d ",i);
            }
            printf("\n");
        }
    }
    return 0;
}
