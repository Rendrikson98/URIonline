#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n1, n2, soma=0, inversor;
    printf("Digite dois valores: ");
    scanf("%d %d", &n1, &n2);
    if (n1 > n2){
            inversor = n2;
            n2=n1;
            n1= inversor;
        }
    for (n1;n1<=n2;n1++){

        if (n1%13 != 0){
            soma +=n1;
        }
    }
    printf("%d", soma);
    return 0;
}
